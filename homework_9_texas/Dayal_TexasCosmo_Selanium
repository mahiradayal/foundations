    "license_box.click()\n",
    "license_box.find_element_by_xpath(\"//option[@value='COS']\").click() \n",
    "nguyen = driver.find_element_by_id(\"pht_lnm\")\n",
    "nguyen.send_keys(\"Nguyen\")\n",
    "submit_button = driver.find_element_by_name(\"B1\")\n",
    "submit_button.click()"
